{
  "expo": {
    "scheme": "mobile-app",
    "name": "mobile-app",
    "slug": "mobile-app",
    "version": "1.0.0",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.yourcompany.mobileapp",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/images/icon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/your-repository-name",
    "extra": {
      "eas": {
        "projectId": "e26f4595-274c-47cb-96cb-c27ae93780bd"
      }
    },
    "plugins": [
    "expo-font"
    ]
  }
}